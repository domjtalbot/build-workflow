{
  "author": "",
  "bugs": {
    "url": "https://github.com/domjtalbot/build-workflow/issues"
  },
  "description": "",
  "homepage": "https://github.com/domjtalbot/build-workflow#readme",
  "license": "ISC",
  "main": "index.js",
  "name": "build-workflow",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domjtalbot/build-workflow.git"
  },
  "scripts": {
    "bootstrap": "npx lerna bootstrap --hoist",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:eslint": "npx eslint .",
    "lint:fix": "npm run lint:eslint:fix",
    "lint": "npm run lint:eslint",
    "postinstall": "npm run bootstrap",
    "test:version:alpha": "npm run version:alpha -- --no-git-tag-version --no-push",
    "test:version:beta": "npm run version:beta -- --no-git-tag-version --no-push",
    "test:version": "npm run version -- --no-git-tag-version --no-push --no-changelog",
    "test:workflow:dry": "npm run test:workflow -- --dryrun",
    "test:workflow:ubuntu": "npm run test:workflow -- --platform ubuntu-latest=nektos/act-environments-ubuntu:18.04",
    "test:workflow": "act pull_request",
    "test": "npx lerna run test --parallel",
    "version:alpha": "npm run version -- prerelease --preid alpha",
    "version:beta": "npm run version -- prerelease --preid beta",
    "version": "npx lerna version --conventional-commits --yes"
  },
  "version": "1.0.0",
  "dependencies": {
    "eslint": "^7.16.0",
    "lerna": "^3.22.1"
  }
}
